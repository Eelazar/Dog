<?xml version="1.0" encoding="utf-8"?>
<root>
  <os name="system">
    <open synonyms="open goto">
      <_node>
        <method>Interact</method>
      </_node>
      <default>
        <debug assistant="2">What do you want to open?</debug>
      </default>
    </open>

    <return synonyms="return">
      <method>MoveUp</method>
    </return>
  </os>

  <open synonyms="view">
    <camera name="Cam">
      <method>OnCommandActivate</method>
    </camera>
    <default>
      <debug assistant="2">What do you want to open?</debug>
    </default>
  </open>

  <move synonyms="move walk go">
    <forward synonyms="forward front up north">
      <method>MoveForward</method>
    </forward>
    <back synonyms="back backwards behind south">
      <method>MoveBack</method>
    </back>
    <left synonyms="left west">
      <method>MoveLeft</method>
    </left>
    <right synonyms="right east">
      <method>MoveRight</method>
    </right>
    <default>
      <debug assistant="2">Where do you want to go?</debug>
    </default>
  </move>

  <throwbro name="throwbro">
    <toss synonyms="toss">
      <method>OnCommandToss</method>
    </toss>
    <default>
      <debug assistant="2">Where do you want to go?</debug>
    </default>
  </throwbro>

  <launch synonyms="launch boot start">
    <_program>
      <method>LaunchProgram</method>
    </_program>
  </launch>
</root>